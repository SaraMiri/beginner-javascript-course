{
  "extends": ["wesbos"],
  "rules": {
    "no-console": 1,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 120,
        "tabWidth": 8
      }
    ]
  },
  "eslint.options": {
    "useEslintrc": false,
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "env": {
      "es6": true
    }
  }
}
